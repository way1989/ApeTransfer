<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SoundWaveView">
        <attr name="waveColor" format="color"/>
        <attr name="soundOriginOrientation">
            <enum name="left" value="1"/>
            <enum name="right" value="0"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MinSizeImageView">
        <attr name="minWidth" format="dimension"/>
        <attr name="minHeight" format="dimension"/>
        <attr name="microSize" format="dimension"/>
    </declare-styleable>
</resources>
